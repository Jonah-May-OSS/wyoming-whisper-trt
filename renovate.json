{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "assignees": ["JonahMMay"],
  "labels": ["renovate"],
  "packageRules": [
    {
      "groupName": "tensorrt",
      "automerge": true,
      "matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
      "matchPackagePatterns": [
        "^tensorrt$",
        "^tensorrt_cu12_bindings$",
        "^tensorrt_cu12_libs$"
      ]
    },
    {
      "automerge": true,
      "matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
      "excludePackagePatterns": [
        "^tensorrt$",
        "^tensorrt_cu12_bindings$",
        "^tensorrt_cu12_libs$"
      ]
    }
  ]
}
